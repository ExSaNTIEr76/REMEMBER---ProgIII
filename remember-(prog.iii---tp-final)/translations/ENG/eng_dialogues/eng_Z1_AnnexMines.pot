#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "Uno de los comederos está más vacío que el otro."
msgstr ""

#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "Parece que tenemos a un pequeño glotón."
msgstr ""

#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "Dentro de las cajas abiertas hay marcas de arañazos."
msgstr ""

#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "En la mesa hay una partida de cartas sin terminar."
msgstr ""

#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "Parece que nadie tenía una buena mano."
msgstr ""

#: dialogues/ZONE 0/Zone0_Ruins.dialogue
msgid "Espera... puedo ver algo escrito detrás de la biblioteca."
